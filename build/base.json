    "tokenColors": [
        {
            "settings": {
                "foreground": "#TEXT#",
                "background": "#BG0#"
            }
        },
        {
            "name": "Comment",
            "scope": [
                "comment",
                "string.quoted.docstring"
            ],
            "settings": {
                "foreground": "#COMMENT#"
            }
        },
        {
            "name": "Strings, User Constants",
            "scope": [
                "string",
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#STRING#"
            }
        },
        {
            "name": "Foreground",
            "scope": [
                "variable",
                "variable.parameter",
                "entity.name.function",
                "entity.name.variable",
                "support.other.variable",
                "support.constant",
                "support.function",
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#TEXT#"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": [
                "keyword",
                "storage",
                "entity.name.tag",
                "support.function.magic.python",
                "keyword.other.use",
                "keyword.other.function.use",
                "keyword.other.namespace",
                "keyword.other.new",
                "keyword.other.special-method",
                "keyword.other.unit",
                "keyword.other.use-as"
