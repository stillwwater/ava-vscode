    "tokenColors": [
        {
            "settings": {
                "foreground": "#C4BCAE",
                "background": "#052329"
            }
        },
        {
            "name": "Comment",
            "scope": [
                "comment",
                "string.quoted.docstring"
            ],
            "settings": {
                "foreground": "#F7F06F"
            }
        },
        {
            "name": "Strings, User Constants",
            "scope": [
                "string",
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#E0EBE7"
            }
        },
        {
            "name": "Foreground",
            "scope": [
